{
  "App": {
    "SelfUrl": "https://localhost:44377",
    "ClientUrl": "http://localhost:4200",
    "CorsOrigins": "https://*.CrossChainServer.com,http://localhost:4200,https://localhost:44336,https://localhost:44389",
    "RedirectAllowedUrls": "http://localhost:4200,https://localhost:44346,https://localhost:44336,https://localhost:44320"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:8080/"
      }
    }
  },
  "AuthServer": {
    "IssuerUri": "http://localhost:8080"
  },
  "ConnectionStrings": {
    "Default": "Server=127.0.0.1;Port=3306;Database=CrossChainServer;Uid=root;Pwd=;Command Timeout=120;"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost",
        "Port": "5672"
      }
    },
    "EventBus": {
      "ClientName": "AuthHandler",
      "ExchangeName": "CrossChainServer"
    }
  },
  "Redis": {
    "Configuration": "127.0.0.1"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "DVb2B8QjyeArjCTY"
  },
  "TimeRange": 1440,
  "ExpirationHour": 48,
  "GraphQL": {
    "Url": "https://dapp-portkey-test.portkey.finance/Portkey_DID/PortKeyIndexerCASchema/graphql",
    "Url2": "https://dapp-aa-portkey-test.portkey.finance/Portkey_V2_DID/PortKeyIndexerCASchema/graphql"
  },
  "ElasticUris": {
    "Uris": [
      "http://127.0.0.1:9200"
    ]
  },
  "IndexSetting": {
    "NumberOfShards": 5,
    "NumberOfReplicas": 1,
    "IndexPrefix": "CrossChainServer"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning",
        "Orleans": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}{Offset:zzz}][{Level:u3}] [{SourceContext}] {Message}{NewLine}{Exception}",
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  },
  "OpenTelemetry": {
    "ServiceName": "CrossChain.AuthServer",
    "ServiceVersion": "1.0.0",
    "CollectorEndpoint": "http://localhost:4315"
  }
}
